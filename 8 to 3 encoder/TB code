module TB();
reg [7:0] in_tb;
wire [2:0] out_tb;

encoder inst(.in(in_tb), .out(out_tb));

initial begin 
#10 in_tb = 8'h80; 
#10;
in_tb = 8'h00;
repeat(8)begin
#10 in_tb = in_tb+01;
end
#10;
$finish;
end
initial begin
$monitor("Time = %0t | in_tb = %b | out_tb = %b ", $time, in_tb, out_tb);
end
endmodule
